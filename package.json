{
  "name": "@signumjs/react-native-expo-crypto-adapter",
  "version": "1.0.4",
  "description": "This is the React Native Expo implementation of the CryptoAdapter for @signumjs/crypto",
  "keywords": [
    "signum",
    "blockchain",
    "crypto",
    "web3",
    "react native",
    "expo"
  ],
  "author": "ohager",
  "private": false,
  "license": "Apache-2.0",
  "main": "./out/index.js",
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "types": "./out/index.d.ts",
      "default": "./out/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "compile": "tsc",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "publish": "./publish.sh"
  },
  "peerDependencies": {
    "react-native": ">=0.60.0",
    "@signumjs/crypto": "^2.0.6",
    "expo": "^53.0.11"
  },
  "dependencies": {
    "aes-js": "^3.1.2",
    "expo-crypto": "~14.1.5",
    "js-sha256": "^0.11.1",
    "react-native-get-random-values": "^1.11.0"
  },
  "devDependencies": {
    "@types/aes-js": "^3.1.4",
    "typescript": "^5.7.2",
    "vitest": "^3.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
